
{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "import",
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": [ 2, "never" ],
    "no-console": [ 1 ],
    "comma-dangle": [ 2, "always-multiline" ]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
