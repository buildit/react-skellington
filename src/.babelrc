{
  "presets": [
    [ "env", {
      "targets": {
        "node": "current",
        "browser": [ "last 2 versions" ]
      },
      "modules": false,
      "debug": true
    }],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    [ "transform-object-rest-spread", { "useBuiltIns": true } ]
  ]
}
